		<script>
			function stopLoader(){
				document.getElementById("appLoading").classList.toggle('dn');
				document.getElementById("appContent").classList.toggle('dn');
			}

			function startLoader(ID){
				if(ID==undefined || ID ==""){
					document.getElementById("appLoading").classList.toggle('dn');
					document.getElementById("appContent").classList.toggle('dn');
				}
			}
		</script>
